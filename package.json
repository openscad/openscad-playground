{
  "name": "openscad-playground",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ochafik.com/openscad2/",
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.3.2",
